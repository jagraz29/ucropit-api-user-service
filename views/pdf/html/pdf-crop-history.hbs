            <div class="page-header-space"></div>
          </td>
        </tr>
      </thead>
      {{! end zona header }}
      {{! contenido }}
      <tbody>
        <tr>
          <td>
            <div class="page end-page">
              <div class="fill">
                {{#switch crop.cropTypeKey}}
                  {{#case "corn"}}
                    <svg class="bigLogoContainer" viewBox="0 0 34 34"></svg>
                    <div>
                      <img
                        class="qr"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/corn.png"
                      />
                    </div>
                  {{/case}}
                  {{#case "soy"}}
                    <svg class="bigLogoContainer" viewBox="0 0 34 34"></svg>
                    <div>
                      <img
                        class="qr"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/soy.png"
                      />
                    </div>
                  {{/case}}
                  {{#case "cotton"}}
                    <svg class="bigLogoContainer" viewBox="0 0 34 34"></svg>
                    <div>
                      <img
                        class="qr"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/cotton.png"
                      />
                    </div>
                  {{/case}}
                  {{#case "carinata"}}
                    <svg class="bigLogoContainer" viewBox="0 0 34 34"></svg>
                    <div>
                      <img
                        class="qr"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/carinata.png"
                      />
                    </div>
                  {{/case}}
                  {{#case "sunflower"}}
                    <svg class="bigLogoContainer" viewBox="0 0 34 34"></svg>
                    <div>
                      <img
                        class="qr"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/sunflower.png"
                      />
                    </div>
                  {{/case}}
                  {{#case "wheat"}}
                    <svg class="bigLogoContainer" viewBox="0 0 34 34"></svg>
                    <div>
                      <img
                        class="qr"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/wheat.png"
                      />
                    </div>
                  {{/case}}
                  {{#default "default"}}
                    <svg class="bigLogoContainer" viewBox="0 0 34 34"></svg>
                    <div>
                      <img
                        class="qr"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/otros.png"
                      />
                    </div>
                  {{/default}}
                {{/switch}}
                <div class="fill-header">
                  <div class="binnacle">
                    Historia del Cultivo
                    <br />
                    impresa el
                    {{moment dateCreatePdf format="DD/MM/YYYY"}}
                  </div>
                  <div>
                    <img
                      src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/ucropit-logo.png"
                      alt="logo-ucropit"
                    />
                  </div>
                </div>
                <div class="dataContainer">
                  <div
                    class="dataValue font-bold"
                    style="width: 300; margin-bottom:10px;"
                  >
                    {{crop.company.name}}
                  </div>
                  <div class="dataValue font-bold">
                    {{crop.cropTypeName.es}}
                  </div>
                  <div class="dataNames">
                    Sup Neta
                  </div>
                  <div class="dataValue">
                    {{crop.surface}}
                    has
                  </div>
                  <div class="dataNames">
                    Fecha Siembra
                  </div>
                  <div class="dataValue">
                    {{crop.dateSowingFormat}}
                  </div>
                  <div class="dataNames">
                    Fecha Cosecha
                  </div>
                  <div class="dataValue">
                    {{crop.dateHarvestFormat}}
                  </div>
                  <div class="dataNames">
                    Rinde
                  </div>
                  <div class="dataValue">
                    {{crop.pay}}
                    Kgs/ha
                  </div>
                  <div>
                    <div class="dataNames">
                      Volumen
                    </div>
                    <div class="dataValue">
                      {{crop.volume}}
                      Ton
                    </div>
                  </div>
                </div>
                <div class="eiq-header">
                  {{#is crop.eiq.range "VERY_LOW"}}
                    <div class="icon-eiq-header color-background-very-low">
                      <img
                        class="img-eiq-header"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/application.svg"
                        width="26"
                        height="26"
                      />
                    </div>
                    <div class="center">
                      EIQ:
                      {{crop.eiq.quantity}}
                      <div class="star-eiq-header color-very-low">
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <p class="label-eiq-severity">
                          Muy Bajo
                        </p>
                      </div>
                    </div>
                  {{/is}}
                  {{#is crop.eiq.range "LOW"}}
                    <div class="icon-eiq-header color-background-low">
                      <img
                        class="img-eiq-header"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/application.svg"
                        width="26"
                        height="26"
                      />
                    </div>
                    <div class="center">
                      EIQ:
                      {{crop.eiq.quantity}}
                      <div class="star-eiq-header color-low">
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <p class="label-eiq-severity">
                          Bajo
                        </p>
                      </div>
                    </div>
                  {{/is}}
                  {{#is crop.eiq.range "MODERATE"}}
                    <div class="icon-eiq-header color-background-moderate">
                      <img
                        class="img-eiq-header"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/application.svg"
                        width="26"
                        height="26"
                      />
                    </div>
                    <div class="center">
                      EIQ:
                      {{crop.eiq.quantity}}
                      <div class="star-eiq-header color-moderate">
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <p class="label-eiq-severity">
                          Moderado
                        </p>
                      </div>
                    </div>
                  {{/is}}
                  {{#is crop.eiq.range "HIGH"}}
                    <div class="icon-eiq-header color-background-height">
                      <img
                        class="img-eiq-header"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/application.svg"
                        width="26"
                        height="26"
                      />
                    </div>
                    <div class="center">
                      EIQ:
                      {{crop.eiq.quantity}}
                      <div class="star-eiq-header color-height">
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <p class="label-eiq-severity">
                          Alto
                        </p>
                      </div>
                    </div>
                  {{/is}}
                  {{#is crop.eiq.range "VERY_HIGH"}}
                    <div class="icon-eiq-header color-background-very-height">
                      <img
                        class="img-eiq-header"
                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/application.svg"
                        width="26"
                        height="26"
                      />
                    </div>
                    <div class="center">
                      EIQ:
                      {{crop.eiq.quantity}}
                      <div class="star-eiq-header color-very-height">
                        <i class="fa fa-star" aria-hidden="true"></i>
                        <p class="label-eiq-severity">
                          Muy Alto
                        </p>
                      </div>
                    </div>
                  {{/is}}
                </div>
                {{#each activities}}
                  {{#if @last}}
                    <div class="lastActivity">
                      <div class="last-frame">
                        <div class="lastActivity-title dataValue no-bold">
                          Status:
                          {{name}}
                          {{status}}
                        </div>
                      </div>
                    </div>
                  {{/if}}
                {{/each}}
                {{! TODO: Insignias }}
                <div class="badges">
                  {{#each crop.badges}}
                    <div class="badges-description">
                      <img src={{badge.imagePath}} class="badges-image" />
                      <div class="hz-description">
                        <p class="size-description">
                          {{badge.name.es}}
                        </p>
                        {{#if surfaceTotal}}
                          <p>
                            {{surfaceTotal}}
                            has
                          </p>
                        {{/if}}
                      </div>
                    </div>
                  {{/each}}
                </div>
              </div>
              {{! TODO: aqui cierra el fill }}
              <div class="crop-leaders">
                <div class="res-contact-header">
                  <h5 class="res-contact-title">
                    Responsable del cultivo
                  </h5>
                  <h3 class="name-header">
                    {{crop.cultivationManager.firstName}}
                    {{crop.cultivationManager.lastName}}
                  </h3>
                  <div class="data-header">
                    {{crop.cultivationManager.phone}}
                  </div>
                  <div class="data-header">
                    {{crop.cultivationManager.email}}
                  </div>
                </div>
                {{#if crop.commercialContact}}
                  <div class="res-contact-header">
                    <h5 class="res-contact-title">
                      Contacto comercial
                    </h5>
                    <h3 class="name-header">
                      {{crop.commercialContact.firstName}}
                      {{crop.commercialContact.lastName}}
                    </h3>
                    <div class="data-header">
                      Vol:
                      {{crop.commercialContact.theoriticalPotential}}
                      ton
                    </div>
                    <div class="data-header">
                      {{crop.commercialContact.phone}}
                    </div>
                    <div class="data-header">
                      {{crop.commercialContact.email}}
                    </div>
                  </div>
                {{/if}}
              </div>
              {{! TODO: aqui cierra la seccion de datos }}
