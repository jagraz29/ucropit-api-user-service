                              <i class="fa fa-check"></i>
                              {{moment dateObservation format="DD/MM/YYYY"}}
                              <span style="float: right;margin-right: 117px;">
                                Validado Por:
                              </span>
                            </div>
                          {{/is}}
                          <div class="activity-description-content">
                            <div>
                              <h3 class="activity-framed-h1">
                                Rinde Estimado:
                                {{pay}}
                                {{unitType}}
