                              <div class="spokesman-data">
                                Evidencias
                              </div>
                              <div class="backup-docs">
                                documentacion de respaldo
                              </div>
                              {{#each evidencesPdf}}
                                <div class="link-pdf">
                                  <img
                                    src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/pdf-icon.svg"
                                    height="25"
                                    width="25"
                                  />
                                  <a href={{path}}>
                                    {{description}}
                                  </a>
                                </div>
                              {{/each}}
                            </div>
                            <div class="activity-validations-list">
                              {{#each signers}}
                                <div>
                                  {{#if dateSigned}}
                                    <div>
                                      <img
                                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/signed.svg"
                                        alt="signed-icon"
                                      />
                                      <span class="validation-date">
                                        {{moment
                                          dateSigned
                                          format="DD/MM/YYYY HH:MM"
                                        }}
                                      </span>
                                    </div>
                                  {{else}}
                                    <div>
                                      <img
                                        src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/signed.svg"
                                        alt="signed-icon"
                                      />
                                      <span class="not-signed-validation-date">
                                        PENDIENTE
                                      </span>
                                    </div>
                                  {{/if}}
                                  <div class="validation-name">
                                    {{fullName}}
                                  </div>
                                  <div class="backup-docs">
                                    {{rol}}
                                    -
                                    {{cuit}}
                                  </div>
                                </div>
                              {{/each}}
                            </div>
                          </div>
                          <div class="activity-frame-lots">
                            {{#if evidenceImages}}
                              {{#each evidenceImages}}
                                <div class="activity-frame-lots-under">
                                  <div class="card">
                                    <img
                                      src={{thumb}}
                                      alt="crop"
                                      class="activity-lots-image"
                                    />
                                    <div class="container">
                                      <h4 class="lot-pic-text">
                                        <b>
                                          {{description}}
                                        </b>
                                      </h4>
                                      {{#if isInLot}}
                                        <p class="type-pic-card">
                                          Dentro del lote
                                        </p>
                                      {{/if}}
                                    </div>
                                  </div>
                                </div>
                              {{/each}}
                            {{/if}}
                          </div>
                        </div>
                      </div>
                    </div>
                  {{else}}
                    <div class="activities-with-achievements">
                      <div class="date-activity-validation">
                        <i class="fa fa-calendar"></i>
                        {{moment dateStart format="DD/MM/YYYY"}}
