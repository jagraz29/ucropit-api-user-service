              {{#each activities}}
                {{#if (or (is status "REALIZADA") (is status "TERMINADA"))}}
                  {{#if (or (is tag "ACT_HARVEST") (is tag "ACT_MONITORING"))}}
                    <div class="activities">
                      <div class="activity-header">
                        <div class="header-container">
                          <div>
                            {{#switch tag}}
                              {{#case "ACT_MONITORING"}}
                                <img
                                  class="activity-type-img"
