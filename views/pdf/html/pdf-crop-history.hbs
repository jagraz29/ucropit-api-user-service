                                  class="activity-type-img"
                                  src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/fertilization.svg"
                                  width="26"
                                  height="26"
                                />
                              {{/case}}
                              {{#case "ACT_TILLAGE"}}
                                <img
                                  class="activity-type-img"
                                  src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/tillage.svg"
                                  width="26"
                                  height="26"
                                />
                              {{/case}}
                            {{/switch}}
                            <div class="activity-title">
                              {{name}}
                            </div>
                          </div>
                          <div>
                            <img
                              src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/ucropit-logo.png"
                              alt="logo-ucropit"
                            />
                          </div>
                        </div>
