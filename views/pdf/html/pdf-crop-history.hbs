                                  width="26"
                                  height="26"
                                />
                              {{/case}}
                              {{#case "ACT_HARVEST"}}
                                <img
                                  class="activity-type-img"
                                  src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/harvest.svg"
                                  width="26"
                                  height="26"
