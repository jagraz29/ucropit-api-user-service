                                  src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/sowing.svg"
                                  width="26"
                                  height="26"
                                />
                              {{/case}}
                              {{#case "ACT_APPLICATION"}}
                                <img
                                  class="activity-type-img"
                                  src="https://ucropsatelliteimagery.s3.amazonaws.com/stage/application.svg"
                                  width="26"
                                  height="26"
                                />
                              {{/case}}
